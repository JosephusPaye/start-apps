{
  "name": "@josephuspaye/start-apps",
  "version": "0.0.0",
  "description": "Get a list of apps in the Start Menu, with icons",
  "main": "dist/main.js",
  "repository": "https://github.com/JosephusPaye/start-apps.git",
  "author": "Josephus Paye II",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "lookup": "dist/main.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"*.{js,json,md}\"",
    "format:check": "prettier --list-different \"src/**/*.ts\" \"*.{js,json,md}\"",
    "test": "echo na"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^14.14.10",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true
  }
}
